' ifelseifelse.vb
Module IfElseIfElse
    Sub Main()
        Dim x As Integer = 5
        If x = 10 Then
            Console.WriteLine("x is 10")
        ElseIf x < 10 Then
            Console.WriteLine("x is less than 10")
        Else
            Console.WriteLine("x is greater than 10")
        End If
    End Sub
End Module
